{
    "rewrites": [
        {
            "source": "/en/:path*",
            "destination": "/en/:path*"
        },
        {
            "source": "/vi/:path*",
            "destination": "/vi/:path*"
        },
        {
            "source": "/:path*",
            "destination": "/en/:path*"
        }
    ],
    "cleanUrls": true
}