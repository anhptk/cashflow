{
    "routes": [
        {
            "src": "/en/(.*)",
            "dest": "/en/$1"
        },
        {
            "src": "/vi/(.*)",
            "dest": "/vi/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/en/$1"
        }
    ],
    "rewrites": [
        {
            "source": "/",
            "destination": "/index.html"
        }
    ]
}